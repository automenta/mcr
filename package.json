{
	"name": "model-context-reasoner",
	"version": "1.0.0",
	"description": "Model Context Reasoner (MCR): Connects Large Language Models (LLMs) with formal logic reasoners (Prolog) for advanced AI applications.",
	"main": "mcr.js",
	"scripts": {
		"start": "node mcr.js",
		"dev": "nodemon mcr.js",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest",
		"test:coverage": "jest --coverage",
		"test:ui": "vitest run --root ui/",
		"test:ui-watch": "vitest --root ui/",
		"test:ui-browser": "vitest --ui --root ui/",
		"dev:ui": "cd ui && vite",
		"build:ui": "vite build ui/",
		"preview:ui": "vite preview ui/",
		"format": "prettier --write .",
		"lint": "eslint . --fix",
		"lint:ui": "eslint ui/",
		"generate-examples": "node src/utility.js --type example",
		"generate-ontology": "node src/utility.js --type ontology",
		"dev:vanilla": "http-server ui/vanilla -p 3001"
	},
	"keywords": [
		"llm",
		"prolog",
		"reasoning",
		"ai",
		"api",
		"nlp",
		"logic programming"
	],
	"author": "Unknown",
	"license": "MIT",
	"type": "commonjs",
	"files": [
		"mcr.js",
		"src/",
		"ontologies/",
		"strategies/",
		"README.md",
		".env.example",
		"LICENSE",
		"generate_example.js",
		"generate_ontology.js",
		"prompts/"
	],
	"repository": {
		"type": "git",
		"url": "http://dumb.ai"
	},
	"bugs": {
		"url": "http://dumb.ai/issues"
	},
	"homepage": "http://dumb.ai#readme",
